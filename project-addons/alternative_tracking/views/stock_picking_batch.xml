<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record model="ir.ui.view" id="stock_batch_picking_form_at">
            <field name="name">stock.picking.batch.form.at</field>
            <field name="model">stock.picking.batch</field>
            <field name="inherit_id" ref="stock_picking_batch_extended.stock_batch_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                   <button name="action_view_stock_move_lines_grouped" class="oe_stat_button" icon="fa-list" type="object">
                            <field string="Picking Ops." name="move_line_grouped_count" widget="statinfo"/>
                        </button>
                </xpath>

            </field>
    </record>
</odoo>
